{
  "name": "movecars",
  "version": "1.0.0",
  "description": "智能挪车通知系统 - Cloudflare Workers",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:worker\"",
    "dev:frontend": "vite",
    "dev:worker": "wrangler dev --port 8787 --local",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:ci": "npm run build && wrangler deploy",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ahooks": "^3.9.6",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "wrangler": "^3.99.0"
  }
}
